{
  "name": "solana-eda",
  "version": "1.0.0",
  "private": true,
  "description": "Solana EDA Trading Bot with Real-time Monitoring",
  "scripts": {
    "setup": "./setup.sh",
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "db:generate": "pnpm --filter @solana-eda/database prisma generate",
    "db:push": "pnpm --filter @solana-eda/database prisma db push",
    "db:migrate": "pnpm --filter @solana-eda/database prisma migrate dev",
    "db:studio": "pnpm --filter @solana-eda/database prisma studio"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.2.5",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
